<div>
	<div class="table-responsive">
		<table class="table table-condensed">
			<thead>
				<tr>
					{{!-- <th>UserID</th> --}}
					<th>First Name</th>
					<th>Last Name</th>
					<th>Email</th>
					<th>Title</th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						{{!-- <td>{{this.id}}</td> --}}
						<td>{{this.firstName}}</td>
						<td>{{this.lastName}}</td>
						<td>{{this.email}}</td>
						<td>{{this.title}}</td>
						</td>
						<td>
							{{!-- <a href="#/{{this._id}}">Edit</a> --}}
							<a href="/deleteUser/{{this._id}}">Delete</a>
						</td>
					</tr>
				{{/each}}
			</tbody>
			<tfooter>
				<tr><td>
				<a href='/addUser'>Add New User</a>
				</td></tr>
				
					
				
			</tfooter>
		</table>
		<a href='/xmlExportUser'>Get XML</a>
		<a href='/jsonExportUser'>Get JSON</a>
	</div>
</div>
