<div>
	<div class="table-responsive">
		<table class="table table-condensed">
			<thead>
				<tr>
					<th>ID</th>
					<th>Title</th>
					<th>Assigned User</th>
					<th>Creation Date</th>
					<th>Start Date</th>
					<th>Completion Date</th>
					<th>Elapsed Time</th>
					<th>Evidence Notes</th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						<td>{{id}}</td>
						<td>{{title}}</td>
						{{#each assignedUserDetails}}
							<td>{{firstName}}</td>
						{{/each}}
						<td>{{creationDate}}</td>
						<td>{{startDate}}</td>
						<td>{{completionDate}}</td>
						<td>{{elapsedTime}} seconds</td>
						<td>
                            {{#each notes}}
								<p>ID:{{id}} 
									{{#each note}} 
										{{title}}: {{text}}
									
								</p>
								{{!-- {{#each note.files}} --}}
									<a href="https://boozallen.taqmanifest.com/{{{this.url}}}">{{this.url}}</a>
								{{!-- {{/each}} --}}
									{{/each}}
							{{/each}}
                        </td>

					</tr>
				{{/each}}
			</tbody>
			<tfooter>
				<tr><td>

				</td></tr>
			</tfooter>
		</table>
	</div>
</div>


